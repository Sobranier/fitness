page {
  background: white;
  height: 100%;
}
page .container {
  height: 100%; overflow: hidden;
}

.fans-item {
  display: -webkit-box;
  padding: 16rpx;
}
.fans-img {
  width: 64rpx; height: 64rpx; margin-top: 12rpx;
}
.fans-img image {
  width: 100%; height: 100%; border-radius: 100%;
}
.fans-name {
  line-height: 88rpx; margin: 0 16rpx;
  -webkit-box-flex: 1;
  font-size: 28rpx;
}