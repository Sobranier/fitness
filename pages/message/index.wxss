page {
  background: white;
  height: 100%;
}
page .container {
  height: 100%; overflow: hidden;
}

.fans-item {
  display: -webkit-box;
  padding: 32rpx 16rpx;
}
.fans-img {
  width: 84rpx; height: 84rpx;
}
.fans-img image {
  width: 100%; height: 100%; border-radius: 100%;
}
.fans-name {
  line-height: 44rpx; margin: 0 16rpx 0 32rpx;
  -webkit-box-flex: 1;
  font-size: 28rpx;
}
.fans-name-title {
  color: #22292c;
}
.fans-name-title text{
  color: #d4d7dd; float: right;
}